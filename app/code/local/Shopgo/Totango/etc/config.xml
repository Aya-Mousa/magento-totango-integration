<?xml version="1.0"?>
<config>
    <modules>
        <Shopgo_Totango>
            <version>1.0.0</version>
        </Shopgo_Totango>
    </modules>
    <global>
        <helpers>
            <totango>
                <class>Shopgo_Totango_Helper</class>
            </totango>
        </helpers>
        <models>
            <totango>
                <class>Shopgo_Totango_Model</class>
            </totango>
        </models>
    </global>
    <default>
        <shopgo_totango>
            <trackers>
                <product>1</product>
                <category>1</category>
                <attribute>1</attribute>
                <shipping_payment>1</shipping_payment>
                <order>1</order>
                <admin_login>1</admin_login>
            </trackers>
        </shopgo_totango>
    </default>
</config>

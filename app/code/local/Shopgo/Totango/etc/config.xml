        <shopgo_totango>
            <trackers>
                <product>1</product>
                <category>1</category>
                <attribute>1</attribute>
                <shipping_payment>1</shipping_payment>
                <order>1</order>
                <admin_login>1</admin_login>
            </trackers>
        </shopgo_totango>
    </default>
</config>
